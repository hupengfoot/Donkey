package com.dianping.donkey;

public class DonkeyClientException extends Exception {

	private static final long serialVersionUID = -2742207187446327514L;

	public DonkeyClientException() {
		super();
	}

	public DonkeyClientException(String message) {
		super(message);
	}

	public DonkeyClientException(String message, Throwable cause) {
		super(message, cause);
	}

	public DonkeyClientException(Throwable cause) {
		super(cause);
	}

}
